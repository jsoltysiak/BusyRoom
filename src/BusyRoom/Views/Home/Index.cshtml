@model IEnumerable<BusyRoom.Models.Room>

@{
    ViewData["Title"] = "Home Page";
}

@foreach (var room in Model)
{
    <div class="row">
        <div class="col-md-12">
            <h2>Room name: @room.Name</h2>
            <p>Number of states: @room.OccupyStates.Count</p>
            <ul>
                @foreach (var occupyState in room.OccupyStates)
                {
                    <li>@occupyState.CreatedOn</li>
                }
            </ul>
        </div>

    </div>
}
